msgstr "Перемкнути обрані записи LUN"

msgid "Toggle Selected LUNs"
msgstr "Перемкнути обрані LUN'и"

msgid "Toggle Selected Listen Records"
msgstr "Перемкнути обрані записи прослуховувань"

msgid "Toggle Selected Media Folders"
msgstr "Перемкнути обрані каталоги медіа"

msgid "Toggle Selected Mutual CHAP Users"
msgstr "Перемкнути обраних взаємних користувачів CHAP"

msgid "Toggle Selected NFS Exports"
msgstr "Перемкнути обрані експорти NFS"

msgid "Toggle Selected NFS Root Directories"
msgstr "Перемкнути обрані кореневы каталоги NFS"

msgid "Toggle Selected Option Records"
msgstr "Перемкнути обрані записи опції"

msgid "Toggle Selected Options"
msgstr "Переключити обрані опції"

msgid "Toggle Selected Port Records"
msgstr "Перемкнути обрані записи порту"

msgid "Toggle Selected Portal Groups"
msgstr "Перемкнути обрані групи порталу"

msgid "Toggle Selected Public Keys"
msgstr "Перемкнути обрані публічні ключі"

msgid "Toggle Selected Records"
msgstr "Перемикання Обраних Записів"

msgid "Toggle Selected Rsync Jobs"
msgstr "Перемикання Обраних Завдань Rsync"

